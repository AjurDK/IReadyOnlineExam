<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />

    

    <link href="~/Template/plugins/jquery-ui/jquery-ui.min.css" rel="stylesheet" />

    <link href="~/Template/plugins/bootstrap/bootstrap4/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Template/plugins/icon/themify-icons/themify-icons.css" rel="stylesheet" />
    <link href="~/Template/css/animate.min.css" rel="stylesheet" />
    <link href="~/Template/css/style.min.css" rel="stylesheet" />
    <link href="~/Template/css/theme/default.css" rel="stylesheet" id="theme" />


    
    <script src="~/Template/plugins/loader/pace/pace.min.js"></script>
    <link href="~/Template/plugins/form/bootstrap-colorpicker/css/bootstrap-colorpicker.min.css" rel="stylesheet" />
    <link href="~/Template/plugins/form/bootstrap-tagsinput/css/bootstrap-tagsinput.css" rel="stylesheet" />
    <link href="~/Template/plugins/form/bootstrap-select/css/bootstrap-select.min.css" rel="stylesheet" />
    <link href="~/Template/plugins/form/bootstrap-slider/css/bootstrap-slider.css" rel="stylesheet" />
    

</head>
<body>
    
    <div id="page-container" class="page-header-fixed page-sidebar-fixed fade">
       
        <div id="header" class="header navbar navbar-default navbar-fixed-top">
            
            <div class="container-fluid">
               
                <div class="navbar-header">
                    <a href="index.html" class="navbar-brand">
                        <i class="ti-infinite navbar-logo text-gradient bg-gradient-blue-purple"></i>
                        <b>IReadyToday</b>Committee Member Dashboard
                    </a>
                    <button type="button" class="navbar-toggle" data-click="sidebar-toggled">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                
                <div class="navbar-xs-justified">
                    <ul class="nav navbar-nav navbar-right">
                        <li>

                        </li>


                        <li class="dropdown">
                            <a href="javascript:;" data-toggle="dropdown">
                                <span class="navbar-user-img online pull-left">
                                    <img src="~/Template/img/user.jpg" alt="" />
                                </span>
                                <span class="hidden-xs ">  @User.Identity.Name <b class="caret"></b></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="@Url.Action("ChangePassword", "Manage")">Change password</a>
                                </li>

                                <li>

                                    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                                    {
                                        @Html.AntiForgeryToken()
                                        <a class="nav-link" href="javascript:document.getElementById('logoutForm').submit()">
                                            <i class="fa fa-lock"></i>
                                            <span class="">Log off </span>
                                        </a>
                                    }

                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>

            </div>

            <div class="header-search-bar">
                <form action="#" method="POST" name="search_bar_form">
                    <div class="form-group">
                        <div class="left-icon"><i class="ti-search"></i></div>
                        <input type="text" class="form-control" id="header-search" placeholder="Search infinite admin..." />
                        <a href="javascript:;" data-dismiss="search-bar" class="right-icon"><i class="ti-close"></i></a>
                    </div>
                </form>
            </div>

        </div>

        <div id="sidebar" class="sidebar sidebar-inverse">





            <div data-scrollbar="true" data-height="100%">

                <ul class="nav">
                    <li class="nav-profile">
                        <div class="image">
                            <img src="~/Template/img/user.jpg" />
                        </div>
                        <div class="info">
                            <h4>  @User.Identity.Name</h4>
                        </div>
                    </li>
                    <li class="nav-divider"></li>

                    <li class="active ti-home"><a href="@Url.Action("Index", "Student")">Home</a></li>


                    <li class="nav-divider"></li>
                    <li class="has-sub">

                    </li>

                    <li>
                        <a href="@Url.Action("Jobmaster","CommitteMember")"><i class="ti-user"></i>Job upload</a>
                    </li>
                    <li>
                        <a href="@Url.Action("StudentAdditionQuestionRequest", "CommitteMember")"><i class="ti-bar-chart"></i>Additional Question Request</a>
                    </li>

                    <li>
                        <a href="@Url.Action("UploadQuizQuestion","CommitteMember")"><i class="ti-user"></i>Upload questions</a>
                    </li>
                    <li>
                        <a href="@Url.Action("StuSyllabus", "CommitteMember")"><i class="ti-bar-chart"></i>Upload syllabus </a>
                    </li>
                    <li>
                        <a href="@Url.Action("FileUpload", "CommitteMember")"><i class="ti-bar-chart"></i>Upload Study Material </a>
                    </li>

                    <li class="nav-divider"></li>

                    <li class="nav-copyright">
                        <p>
                            Copyright ©
                            <script language="JavaScript" type="text/javascript">document.write((new Date()).getFullYear());</script> - Designed and Hosted by <a href="http://shlrtechnosoft.com/" target="_blank">SHLR Technosoft PVT LTD</a>
                        </p>
                    </li>
                </ul>

            </div>

            <a href="#" class="sidebar-minify-btn" data-click="sidebar-minify"><i class="ti-arrow-left"></i></a>

        </div>

        <div id="content" class="content">

            @RenderBody()
        </div>

        <a href="#" data-click="scroll-top" class="btn-scroll-top fade"><i class="ti-arrow-up"></i></a>

    </div>


    <script src="~/Template/plugins/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/Template/plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Template/plugins/cookie/js/js.cookie.js"></script>
    <script src="~/Template/plugins/tooltip/popper/popper.min.js"></script>
    <script src="~/Template/plugins/bootstrap/bootstrap4/js/bootstrap.min.js"></script>
    <script src="~/Template/plugins/scrollbar/slimscroll/jquery.slimscroll.min.js"></script>



    <script src="~/Template/plugins/chart/chart-js/Chart.min.js"></script>
    <script src="~/Template/js/page/dashboard.demo.min.js"></script>
    <script src="~/Template/js/apps.min.js"></script>

    <script src="~/Template/plugins/form/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
    <script src="~/Template/plugins/form/bootstrap-typeahead/js/bootstrap-typeahead.min.js"></script>
    <script src="~/Template/plugins/form/bootstrap-tagsinput/js/bootstrap-tagsinput.min.js"></script>
    <script src="~/Template/plugins/form/bootstrap-slider/js/bootstrap-slider.min.js"></script>
    <script src="~/Template/plugins/form/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/Template/plugins/form/masked-input/js/masked-input.min.js"></script>
    <script src="~/Template/plugins/form/pwstrength/js/pwstrength.js"></script>
    <script src="~/Template/js/page/form-plugins.demo.min.js"></script>
    <script src="~/Template/js/apps.min.js"></script>



    <script>
        $(document).ready(function () {
            App.init();
            Dashboard.init();
        });
    </script>
</body>
</html>
