@model IEnumerable<IReadyonlineexam.Models.MyQuizModel>

@{
    ViewBag.Title = "QuizList";
    Layout = "~/Views/Contributor/Shared/_ContributorLayoutPage.cshtml";
}

<h2>QuizList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuizName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.SubjectName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.UnitName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
    </tr>

@foreach (var item in ViewBag.QuizList) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QuizName)
        </td>     
        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnitName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
    </tr>
}

</table>
