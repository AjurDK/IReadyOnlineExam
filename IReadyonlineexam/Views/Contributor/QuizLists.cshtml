@model IEnumerable<IReadyonlineexam.Models.MyQuizModel>

<div class="panel-body">
    <table id="datatables-default" class="table table-striped table-condensed table-bordered bg-white">
        <thead>
            <tr>
                <th>
                    SL NO
                </th>
                <th>
                    Category
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CourseName)
                </th>

                <th>
                    Add Questions
                </th>
                <th>
                    Upload Excel
                </th>
            </tr>
        </thead>
        <tbody>
            @{int i = 0; }
            @foreach (var item in Model)
            {
                i++;
                <tr>
                    <td>@i</td>
                    <td>
                        @Html.DisplayFor(modelItem => item.QuizName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CourseName)
                    </td>
                    <td>
                        @Html.ActionLink("Click Here", "QuestionInsert", new { _quizID = item.QuizID })
                    </td>
                    <td>
                        
                        @Html.ActionLink("Click Here", "QuestionUpload", new { _quizID = item.QuizID })

                    </td>
                    <td>
                        @Html.ActionLink("Delete", "QuizDelete", new { _QuizID = item.QuizID }) |
                        @Html.ActionLink("Details", "QuizDetails", new { _QuizID = item.QuizID }) |

                    </td>
                </tr>
            }
        </tbody>


    </table>
</div>