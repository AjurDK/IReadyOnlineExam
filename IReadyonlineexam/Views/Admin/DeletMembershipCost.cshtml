@model IReadyonlineexam.Models.StuMembershipCost_master

@{
    ViewBag.Title = "DeletMembershipCost";
    Layout = "~/Views/Admin/Shared/_AdminLayoutPage.cshtml";
}

<h4>Delete Membership Cost</h4>
<h3>Are you sure you want to delete this?</h3>
@if (ViewBag.result != null)
{
    <script>
        alert("@ViewBag.result");
    </script>
}

<div id="rootwizard" class="wizard wizard-full-width">

    <form action="#" method="POST" name="wizard_form">

        <div class="wizard-content tab-content">

            <div class="tab-pane active" id="tab1">
                @using (Html.BeginForm("DeletMembershipCost", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @Html.HiddenFor(model => model.MemID)

                    <div class="row">

                        <div class="col-md-6 offset-md-3">
                            <p class="desc m-b-20"></p>
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })





                            <div class="form-group">
                                <label class="control-label">Membership Cost<span class="text-danger">*</span></label>
                                @Html.EditorFor(model => model.membershipcost, new { htmlAttributes = new { @class = "form-control" } })
                            </div>



                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Delete" class="btn btn-primary" />
                                </div>
                            </div>

                        </div>
                    </div>

                }
                <div>
                    @Html.ActionLink("Back to List", "membershipcostadd")
                </div>
            </div>
        </div>

    </form>

</div>




